lf: flex-start;\r\n    }\r\n\r\n    .text-info{\r\n        margin: 5vh;\r\n        padding-left: 2rem;\r\n        align-self: flex-end;\r\n        text-shadow: 1px 1px 2px $color0;\r\n                \r\n        @mixin blending{\r\n            mix-blend-mode: hard-light;\r\n            \r\n        }\r\n    \r\n        h2 {\r\n            margin-top: 2rem;        \r\n            @include blending;\r\n        }\r\n        h3 {\r\n            margin: 2rem;\r\n            @include blending;\r\n        }\r\n        h1{\r\n            @include blending;\r\n            font-size: 2rem;\r\n        }\r\n    }\r\n}\r\n\r\n.work {\r\n    text-align: center;\r\n    background-color: $color80;\r\n    padding-top: 3rem;\r\n\r\n    h2,\r\n    h4,\r\n    h5,\r\n    p {\r\n        color: $color20;\r\n    }\r\n\r\n    .card-container {\r\n        margin-top: 4rem;\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        align-items: center;\r\n\r\n        .card {\r\n            background-color: $color100;\r\n            margin: 1rem;\r\n            width: 100vw;\r\n            height: 40vh;\r\n            @include shadow;\r\n            position: relative;\r\n        }\r\n\r\n        img {\r\n            display: block;\r\n            @include shadow;\r\n            width: $small-img-size;\r\n            height: $small-img-size;\r\n            border-radius: 50%;\r\n            border: 2px solid $color100;\r\n            position: absolute;\r\n            top: -($small-img-size/2);\r\n            left: 0;\r\n            right: 0;\r\n            margin-left: auto;\r\n            margin-right: auto;\r\n        }\r\n\r\n        h4 {\r\n            margin-top: 56px;\r\n        }\r\n\r\n        h5 {\r\n            font-style: italic;\r\n            margin: 0.25rem;\r\n        }\r\n        p {\r\n            font-size: $small-font;\r\n        }\r\n    }\r\n\r\n    @media only screen and (max-width: 768px){\r\n        .work{\r\n            he